click-right = ~/dotfiles/.config/polybar/scripts/battery.sh prev
format-background = ${root.battery-color}
format-foreground = ${root.battery-text-color}
label-underline = ${root.date-underline}
label-overline = ${root.date-overline}
format-prefix = ${root.battery-prefix}
format-prefix-font = 6
format-prefix-foreground = ${root.battery-prefix-foreground}
format-prefix-background = ${root.battery-prefix-background}
format-suffix = ${root.battery-suffix}
format-suffix-font = 6
format-suffix-foreground = ${root.battery-suffix-foreground}
format-suffix-background = ${root.battery-suffix-background}
