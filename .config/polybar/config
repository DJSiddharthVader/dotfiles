label-focused-overline = ${module/bspwm.label-focused-overline}
label-focused-padding = ${module/bspwm.label-focused-padding}
; unfocused = Inactive workspace on any monitor
label-unfocused = %icon%
label-unfocused-background = ${module/bspwm.label-occupied-background}
label-unfocused-foreground = ${module/bspwm.label-occupied-foreground}
label-unfocused-underline = ${module/bspwm.label-occupied-underline}
label-unfocused-overline = ${module/bspwm.label-occupied-overline}
label-unfocused-padding = ${module/bspwm.label-occupied-padding}
;visible = Active workspace on unfocused monitor
label-visible = %icon%
label-visible-background = ${module/bspwm.label-empty-background}
label-visible-foreground = ${module/bspwm.label-empty-foreground}
label-visible-underline = ${module/bspwm.label-empty-underline}
label-visible-overline = ${module/bspwm.label-empty-overline}
label-visible-padding = 1
; urgent = Workspace with urgency hint set
label-urgent = %icon%
label-urgent-background = ${module/bspwm.label-urgent-background}
label-urgent-foreground = ${module/bspwm.label-urgent-foreground}
label-urgent-underline = ${module/bspwm.label-urgent-underline}
label-urgent-overline = ${module/bspwm.label-urgent-overline}
label-urgent-padding = ${module/bspwm.label-urgent-padding}
format = <label-state> <label-mode>
label-underline = ${root.i3-underline}
label-overline = ${root.i3-overline}
format-prefix = ${root.i3-prefix}
format-prefix-font = 6
format-prefix-foreground = ${root.i3-prefix-foreground}
format-prefix-background = ${root.i3-prefix-background}
format-suffix = ${root.i3-suffix}
format-suffix-font = 6
format-suffix-foreground = ${root.i3-suffix-foreground}
format-suffix-background = ${root.i3-suffix-background}
