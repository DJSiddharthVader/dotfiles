69,79fold
let &fdl = &fdl
let s:l = 69 - ((63 * winheight(0) + 13) / 26)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 69
normal! 01|
let &g:so = s:so_save | let &g:siso = s:siso_save
